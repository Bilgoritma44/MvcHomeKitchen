@using MvcHomeKitchen.Models.Concrete
@model List<Recipe>
<div class="tech-btm">
    <h4>Son Ekelenen Tarifler</h4>
    @foreach (var x in Model)
    {
    <div class="blog-grids row mb-3">
        <div class="col-md-5 blog-grid-left">
            <a href="single.html">
                <img src="@x.Url" class="img-fluid" alt="">
            </a>
        </div>
        <div class="col-md-7 blog-grid-right">

            <h5>
                <a href="single.html">@x.Context </a>
            </h5>
            <div class="sub-meta">
                <span>
                    <i class="fas fa-user"></i> @x.Writer.Name @x.Writer.Surname
                </span>
            </div>
        </div>

    </div>
    }
</div>